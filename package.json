{
  "name": "web-mentor",
  "version": "1.0.0",
  "description": "CMS grading for mentors",
  "main": "src/server.ts",
  "scripts": {
    "minify": "rimraf ./public && gulp minify-files",
    "minify:watch": "gulp watch-files",
    "build": "rimraf ./dist && npx tsc -b",
    "dev": "npm run minify && cross-env NODE_ENV=development ts-node-dev src/server.ts",
    "start": "npm run minify && cross-env NODE_ENV=developmentts-node src/server.ts",
    "check": "prettier --check \"src/**/*.ts\" ",
    "format": "prettier --write \"src/**/*.ts\" ",
    "deploy": "npm run minify && npm run build && rimraf ./deploy && webpack",
    "image": "docker build -t web-mentor-image . ",
    "test": "docker run -p 8080:8080 --name web-mentor web-mentor-image"
  },
  "keywords": [
    "CMS",
    "Grading",
    "Mentor",
    "Student"
  ],
  "author": "HelloGIT team",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/connect-flash": "^0.0.37",
    "@types/csrf": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/express-ejs-layouts": "^2.5.0",
    "@types/express-session": "^1.17.4",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/method-override": "^0.0.32",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.3",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.9",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/qrcode": "^1.4.2",
    "@types/serve-favicon": "^2.5.3",
    "@types/uuid": "^8.3.4",
    "cross-env": "^7.0.3",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "7.1.0",

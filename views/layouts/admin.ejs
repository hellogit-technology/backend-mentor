<!DOCTYPE html>
<html lang="en">
	<!--begin::Head-->
	<head>
		<meta charset="utf-8" />
		<title><%= payload.infoPage.title %></title>
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,600,700" />
		<link rel="stylesheet" href="/lib/font-awesome/css/all.min.css">
		<link rel="stylesheet" href="/lib/font-awesome/css/fontawesome.min.css">
		<link rel="stylesheet" href="/global/global.min.css">
		<link rel="stylesheet" href="/css/validation.min.css">
		<link rel="stylesheet" href="/css/toast.min.css">
	</head>
	<!--end::Head-->
	<!--begin::Body-->
	<body id="kt_body" class="header-fixed header-tablet-and-mobile-fixed aside-fixed">
		<!--begin::Main-->
		<div class="d-flex flex-column flex-root">
			<!--begin::Page-->
			<div class="page d-flex flex-row flex-column-fluid">

				<!--begin::Aside-->
				<%- include('../components/admin/sidebar.ejs') %>
				<!--end::Aside-->

				<!--begin::Wrapper-->
				<%- body %>
				<!--end::Wrapper-->

			</div>
			<!--end::Page-->
		</div>
		<!--end::Main-->
		<div class="modal fade" id="pdp_edit_info" tabindex="-1" aria-hidden="true">
			<!--begin::Modal dialog-->
			<div class="modal-dialog modal-dialog-centered mw-650px">
				<!--begin::Modal content-->
				<div class="modal-content rounded">
					<!--begin::Modal header-->
					<div class="modal-header pb-0 border-0 justify-content-end">
						<!--begin::Close-->
						<div class="btn btn-sm btn-icon btn-active-color-primary" data-bs-dismiss="modal">
							<!--begin::Svg Icon | path: icons/duotune/arrows/arr061.svg-->
							<span class="svg-icon svg-icon-1">
								<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
									<rect opacity="0.5" x="6" y="17.3137" width="16" height="2" rx="1" transform="rotate(-45 6 17.3137)" fill="black" />
									<rect x="7.41422" y="6" width="16" height="2" rx="1" transform="rotate(45 7.41422 6)" fill="black" />
								</svg>
							</span>
							<!--end::Svg Icon-->
						</div>
						<!--end::Close-->
					</div>
					<!--begin::Modal header-->
					<!--begin::Modal body-->
					<div class="modal-body scroll-y px-10 px-lg-15 pt-0 pb-15">
						<!--begin:Form-->
						<form id="kt_modal_evaluation_form" class="form" action="#">
							<!--begin::Heading-->
							<div class="mb-13 text-center">
								<!--begin::Title-->
								<h1 class="mb-3">Chỉnh sửa thông tin cá nhân</h1>
								<!--end::Title-->
							</div>
							<!--end::Heading-->				
							<!--begin::Input group-->
							<div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
								<!--begin::Label-->
								<label class="d-flex align-items-center fs-6 fw-bold mb-2">
									<span>Họ và tên: </span>
								</label>
								<!--end::Label-->
								<input type="text" class="form-control form-control-solid" placeholder="Phạm Nguyễn Minh Toàn" />
							</div>
							<!--end::Input group-->						
                            <!--begin::Input group-->
							<div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
								<!--begin::Label-->
								<label class="d-flex align-items-center fs-6 fw-bold mb-2">
									<span>Email:</span>
								</label>
								<!--end::Label-->
								<input type="text" class="form-control form-control-solid" placeholder="toanpnm@fe.edu.vn" />
							</div>
							<!--end::Input group-->
							<!--begin::Input group-->
							<div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
								<!--begin::Label-->
								<label class="d-flex align-items-center fs-6 fw-bold mb-2">
									<span>SĐT:</span>
								</label>
								<!--end::Label-->
								<input type="text" class="form-control form-control-solid" placeholder="0123456789" />
							</div>
							<!--end::Input group-->
							<!--begin::Input group-->
							<div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
								<!--begin::Label-->
								<label class="d-flex align-items-center fs-6 fw-bold mb-2">
									<span>Campus:</span>
								</label>
								<!--end::Label-->
								<input type="text" class="form-control form-control-solid" placeholder="Hồ Chí Minh"/>
							</div>
							<!--end::Input group-->	
							<!--begin::Input group-->
							<div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
								<!--begin::Label-->
								<label class="d-flex align-items-center fs-6 fw-bold mb-2">
									<span>Facebook:</span>
								</label>
								<!--end::Label-->
								<input type="text" class="form-control form-control-solid" placeholder="https://www.facebook.com/toanpham70411"/>
							</div>
							<!--end::Input group-->	
							<!--begin::Input group-->
							<div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
								<!--begin::Label-->
								<label class="d-flex align-items-center fs-6 fw-bold mb-2">
									<span>Avatar:</span>
								</label>
								<!--end::Label-->
								<input type="file" class="form-control form-control-solid" />
							</div>
							<!--end::Input group-->															
                            <!--begin::Actions-->
							<div class="text-center">
								<button type="reset" id="kt_modal_new_target_cancel" class="btn btn-light me-3">Huỷ</button>
								<button type="submit" id="kt_modal_new_target_submit" class="btn btn-primary">
									<span class="indicator-label">Lưu</span>
									<span class="indicator-progress">Đang xử lý...
									<span class="spinner-border spinner-border-sm align-middle ms-2"></span></span>
								</button>
							</div>
							<!--end::Actions-->
						</form>
						<!--end:Form-->
					</div>
					<!--end::Modal body-->
				</div>
				<!--end::Modal content-->
			</div>
			<!--end::Modal dialog-->
		</div>
		<!--end::Modal - Edit info-->
		<!--begin::Modal - Detail Event-->
		<div class="modal fade" id="pdp_detail_event" tabindex="-1" aria-hidden="true">
			<!--begin::Modal dialog-->
			<div class="modal-dialog modal-dialog-centered mw-650px">
				<!--begin::Modal content-->
				<div class="modal-content rounded">
					<!--begin::Modal header-->
					<div class="modal-header pb-0 border-0 justify-content-end">
						<!--begin::Close-->
						<div class="btn btn-sm btn-icon btn-active-color-primary close-detail-event-modal" data-bs-dismiss="modal">
							<!--begin::Svg Icon | path: icons/duotune/arrows/arr061.svg-->
							<span class="svg-icon svg-icon-1">
								<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
									<rect opacity="0.5" x="6" y="17.3137" width="16" height="2" rx="1" transform="rotate(-45 6 17.3137)" fill="black" />
									<rect x="7.41422" y="6" width="16" height="2" rx="1" transform="rotate(45 7.41422 6)" fill="black" />
								</svg>
							</span>
							<!--end::Svg Icon-->
						</div>
						<!--end::Close-->
					</div>
					<!--begin::Modal header-->
					<!--begin::Modal body-->
					<div class="modal-body scroll-y px-10 px-lg-15 pt-0 pb-15">
						<!--begin:Form-->
						<form id="kt_modal_evaluation_form_detail_event" class="form">
							<!--begin::Heading-->
							<div class="mb-13 text-center">
								<!--begin::Title-->
								<h1 class="mb-3">Chi tiết sự kiện</h1>
								<!--end::Title-->
							</div>
							<!--end::Heading-->		
							<div class="row mb-8">
								<label class="d-flex align-items-center fs-6 fw-bold mb-2">
									<span>ID:</span>
								</label>
								<div class="row">
									<div class="col-md-12 fv-row">
										<output type="text" id="eventId" class="form-control form-control-solid" style="margin-bottom:10px;"></output>
									</div>
								</div>
							</div>
							<!--end::Input group-->			
							<!--begin::Input group-->
							<div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
								<!--begin::Label-->
								<label class="d-flex align-items-center fs-6 fw-bold mb-2">
									<span>Tên sự kiện: </span>
								</label>
								<!--end::Label-->
								<output type="text" id="eventName" class="form-control form-control-solid"></output>
							</div>
							<!--end::Input group-->
                            <!--begin::Input group-->
							<div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
								<!--begin::Label-->
								<label class="d-flex align-items-center fs-6 fw-bold mb-2">
									<span>Ngày diễn ra:</span>
								</label>
								<!--end::Label-->
                                <output type="text" id="eventDate" class="form-control form-control-solid"></output>
							</div>
							<!--end::Input group-->
							<!--begin::Input group-->
							<div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
								<!--begin::Label-->
								<label class="d-flex align-items-center fs-6 fw-bold mb-2">
									<span>Cơ sở: </span>
								</label>
								<!--end::Label-->
								<output type="text" id="campus" class="form-control form-control-solid"></output>
							</div>
							<!--end::Input group-->
                             <!--begin::Input group-->
							<div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
								<!--begin::Label-->
								<label id="event-clubs" class="d-flex align-items-center fs-6 fw-bold mb-2">
									<span>Đồng tổ chức</span>
								</label>
								<!--end::Label-->
								
							</div>
							<!--end::Input group-->
							<!--begin: upload eventPoster-->
							<div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
								<!--begin::Label-->
								<label class="d-flex align-items-center fs-6 fw-bold mb-2">
									<span>Poster sự kiện:</span>
								</label>
								<!--end::Label-->
								<div class="text-center" style="margin-bottom: 15px; ">
									<img class="text-center" style="border: 1px solid #f6f2f2; max-width: 550px;max-height: 310px;min-width: 550px;min-height: 310px;" id="poster" src="#" alt="poster" />
								</div>
							</div>
							<!--end: upload eventPoster-->		
                            <div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
								<!--begin::Label-->
								<label class="d-flex align-items-center fs-6 fw-bold mb-2">
									<span>QR Code:</span>
								</label>
								<!--end::Label-->
								<div class="text-center">
                                    <img src="#" id="qrcode" width="150px;" height="150px" alt="qrcode">
                                </div>
							</div>
							<!--begin: en-disable QR-->
							<div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
								<!--begin::Label-->
								<label class="d-flex align-items-center fs-6 fw-bold mb-2">
									<span>Vô hiệu hoá QR: </span>
									<span class="form-check form-switch form-check-custom form-check-solid" style="margin-left: 10px;">
										<input class="form-check-input" type="checkbox" value="" id="switchExpire"/>
									</span>
								</label>
								<!--end::Label-->
							</div>
							<!--end: en-disable QR-->
                            <!--begin::Actions-->
							<div class="text-center">
								<button id="kt_modal_new_target_cancel" data-bs-dismiss="modal" class="btn btn-light me-3 close-detail-event-modal">Thoát</button>    
                                <button id="pdp_download_qr" class="btn btn-success me-3">Download QR</button>                         
							</div>
							<!--end::Actions-->
						</form>
						<!--end:Form-->
					</div>
					<!--end::Modal body-->
				</div>
				<!--end::Modal content-->
			</div>
			<!--end::Modal dialog-->
		</div>
		<!--end::Modal - Detail Event-->
		
		<!--begin::Scrolltop-->
		<div id="kt_scrolltop" class="scrolltop rounded-circle" data-kt-scrolltop="true">
			<!--begin::Svg Icon | path: icons/duotone/Navigation/Up-2.svg-->
			<span class="svg-icon">
				<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
					<g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
						<polygon points="0 0 24 0 24 24 0 24" />
						<rect fill="#000000" opacity="0.5" x="11" y="10" width="2" height="10" rx="1" />
						<path d="M6.70710678,12.7071068 C6.31658249,13.0976311 5.68341751,13.0976311 5.29289322,12.7071068 C4.90236893,12.3165825 4.90236893,11.6834175 5.29289322,11.2928932 L11.2928932,5.29289322 C11.6714722,4.91431428 12.2810586,4.90106866 12.6757246,5.26284586 L18.6757246,10.7628459 C19.0828436,11.1360383 19.1103465,11.7686056 18.7371541,12.1757246 C18.3639617,12.5828436 17.7313944,12.6103465 17.3242754,12.2371541 L12.0300757,7.38413782 L6.70710678,12.7071068 Z" fill="#000000" fill-rule="nonzero" />
					</g>
				</svg>
			</span>
			<!--end::Svg Icon-->
		</div>

		<!-- Show toast -->
		<div id="show-toast"></div>
		
		<script src="/global/global.bundle.min.js"></script>
		<script src="/js/jquery.validate.min.js"></script>
		<script src="/js/additional-methods.min.js"></script>
		<script src="/js/toast.bundle.min.js"></script>
		<script src="/js/lodash.min.js"></script>
		<script src="/js/methods.bundle.min.js"></script>
		<script src="/js/validation-ad23.bundle.min.js"></script>
	</body>
	<!--end::Body-->
</html>
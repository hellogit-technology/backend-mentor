    <!--begin::Content-->
    <div class="content d-flex flex-column flex-column-fluid fs-6" id="kt_content">		
        <!--begin::Student--> 
        <div class="e-panel card" style="margin-left:10px; margin-right: 10px;" >
            <div class="card-body" style="font-size:15px;">
                <div class="e-table">
                    <div class="table-responsive table-lg mt-3">
                        <table class="table table-bordered">
                            <% if (typeof(student) !== 'undefined') { %>
                                <% if (student.length > 0) { %>
                                    <thead>
                                        <tr class="fw-bold fs-6 text-gray-800 border-bottom border-gray-200">
                                            <th style="width:100px;">Stt</th>
                                            <th style="width:100px;">ID</th>
                                            <th style="width:300px;">Họ và tên</th>
                                            <th style="width:250px;">Email</th>
                                            <th style="width:250px;">Cơ sở đang học</th>
                                            <th style="width:200px;" class="text-center">Được thêm bởi</th>
                                            <th style="width:200px;" class="text-center">Sửa/Xoá</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% student.forEach((person, index) => { %> 
                                            <tr> 
                                                <td class="text-nowrap align-middle"><%= index + 1 %></td>
                                                <td class="text-nowrap align-middle"><%= person.schoolId %></td>
                                                <td class="text-nowrap align-middle"><%= person.fullname %></td>
                                                <td class="text-nowrap align-middle"><%= person.email %></td>
                                                <td class="text-nowrap align-middle"><%= person.campus.name %></td>
                                                <td class="text-center align-middle">
                                                    <a href="#">
                                                        <%= person.editor.fullname %>
                                                    </a>
                                                </td>
                                                <td class="text-center align-middle">
                                                    <div class="btn-group align-top">
                                                        <button class="btn btn-secondary badge update-student" type="button" data-id="<%= person._id %>" data-schoolId="<%= person.schoolId %>" data-fullname="<%= person.fullname %>" data-email="<%= person.email %>" data-campus="<%= person.campus %>" data-bs-toggle="modal" data-bs-target="#pdp_edit_student">Sửa</button>
                                                        <button class="btn btn-secondary badge remove-student" data-id="<%= person._id %>" type="button" data-bs-toggle="modal" data-bs-target="#delete_student"><i class="fa fa-trash"></i></button>
                                                    </div>
                                                </td>
                                            </tr>
                                        <% }) %> 
                                    </tbody>
                                <% } else { %>
                                    <div class="row g-3 text-center">
                                        <h3>No Record</h3>
                                    </div>
                                <% } %> 
                            <% } %>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <!--end::Student-->
    </div>
    <!--end::Content-->
    <!--begin::Show result-->
    <%- include('../../partials/pagination.ejs') %>
    <!--end::Show result-->
    <!--begin::Footer-->
    <%- include('../../partials/footer.ejs') %>
    <!--end::Footer-->
</div>

<!--begin::Modal - Edit Student-->
<div class="modal fade" id="pdp_edit_student" tabindex="-1" aria-hidden="true">
    <!--begin::Modal dialog-->
    <div class="modal-dialog modal-dialog-centered mw-650px">
        <!--begin::Modal content-->
        <div class="modal-content rounded">
            <!--begin::Modal header-->
            <div class="modal-header pb-0 border-0 justify-content-end">
                <!--begin::Close-->
                <div class="btn btn-sm btn-icon btn-active-color-primary modal_student_cancel" data-bs-dismiss="modal">
                    <!--begin::Svg Icon | path: icons/duotune/arrows/arr061.svg-->
                    <span class="svg-icon svg-icon-1">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                            <rect opacity="0.5" x="6" y="17.3137" width="16" height="2" rx="1" transform="rotate(-45 6 17.3137)" fill="black" />
                            <rect x="7.41422" y="6" width="16" height="2" rx="1" transform="rotate(45 7.41422 6)" fill="black" />
                        </svg>
                    </span>
                    <!--end::Svg Icon-->
                </div>
                <!--end::Close-->
            </div>
            <!--begin::Modal header-->
            <!--begin::Modal body-->
            <div class="modal-body scroll-y px-10 px-lg-15 pt-0 pb-15">
                <!--begin:Form-->
                <form id="modal_update_student_form" class="form">
                    <!--begin::Heading-->
                    <div class="mb-13 text-center">
                        <!--begin::Title-->
                        <h1 class="mb-3">Chỉnh sửa thông tin sinh viên</h1>
                        <!--end::Title-->
                    </div>
                    <!--end::Heading-->
                    <!--begin::Input group-->
                    <div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
                        <!--begin::Label-->
                        <label class="d-flex align-items-center fs-6 fw-bold mb-2">
                            <span>ID:</span>
                        </label>
                        <!--end::Label-->
                        <input type="text" id="schoolId" class="form-control form-control-solid" />
                    </div>
                    <!--end::Input group-->							
                    <!--begin::Input group-->
                    <div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
                        <!--begin::Label-->
                        <label class="d-flex align-items-center fs-6 fw-bold mb-2">
                            <span>Họ và tên: </span>
                        </label>
                        <!--end::Label-->
                        <input type="text" id="fullname" class="form-control form-control-solid" />
                    </div>
                    <!--end::Input group-->
                    <!--begin::Input group-->
                    <div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
                        <!--begin::Label-->
                        <label class="d-flex align-items-center fs-6 fw-bold mb-2">
                            <span>Email:</span>
                        </label>
                        <!--end::Label-->
                        <input type="text" id="email" class="form-control form-control-solid">
                    </div>
                    <!--end::Input group-->
                    <!--begin::Input group-->
                    <div class="d-flex flex-column mb-8 fv-row" style="margin-top: 30px;">
                        <!--begin::Label-->
                        <label class="d-flex align-items-center fs-6 fw-bold mb-2">
                            <span>Cơ sở đang học:</span>
                        </label>
                        <!--end::Label-->
                        <select name="campus" id="campus" aria-label="Select a Campus" data-control="select_campus" data-placeholder="Campus" class="form-select form-select-solid">
                            <option value="" selected="" disabled="">Chọn cơ sở</option>
                            <% if (typeof(campus) !== 'undefined') { %>
                                <% campus.forEach(value => { %>
                                    <option value="<%= value['_id'] %>"><%= value['name'] %></option>
                                <% }) %> 
                            <% } %>
                        </select>
                    </div>
                    <!--end::Input group-->
                    <!--begin::Actions-->
                    <div class="text-center">
                        <button type="button" data-bs-dismiss="modal" class="btn btn-light me-3 modal_student_cancel">Huỷ</button>
                        <button type="submit" id="kt_modal_new_target_submit" style="background-color: #fdb32c;" class="btn btn-primary">Cập nhật</button>
                    </div>
                    <!--end::Actions-->
                </form>
                <!--end:Form-->
            </div>
            <!--end::Modal body-->
        </div>
        <!--end::Modal content-->
    </div>
    <!--end::Modal dialog-->
</div>
<!--end::Modal - Edit Student-->  

